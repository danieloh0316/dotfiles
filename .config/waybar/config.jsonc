{
    "layer": "top",
    "output": "DP-3",
    "position": "top",
    "height": 40,
    "modules-left": [
        "hyprland/workspaces"
    ],
    "modules-center": [
        "clock#clock",
        "clock#date"
    ],
    "modules-right": [
        "custom/rss",
        "battery",
        "network"
    ],
    "battery": {
        "format": "{icon}",
        "format-icons": [
            "",
            "",
            "",
            "",
            ""
        ]
    },
    "clock#clock": {
        "format": " {:%R}"
    },
    "clock#date": {
        "format": " {:%b %d}"
    },
    "hyprland/workspaces": {
        "all-outputs": true
    },
    "network": {
        "interface": "wlp59s0",
        "format-wifi": "󰤨",
        "format-disconnected": "󰤭",
        "tooltip-format-wifi": "{essid} ({signalStrength}%)"
    },
    "custom/rss": {
        "exec": "./.config/waybar/scripts/rss",
        "format": "󰑬 {}",
        "interval": 3600,
        "tooltip": false
    }
}
